    <script type="text/javascript">
    var title = '{$pic}';
    var hole = parseInt('{$hole}');
    var rows = parseInt('{$rows}');
    var cols = parseInt('{$cols}');
    var rounds = parseInt('{$rounds}');
    {literal}
        //成功闯关人数轮播
        $('.marquee').marquee({
            duplicated:true,
            speed:70
        });
        $(function(){
        	var secodes = 0;
        	var settings = {
        		rows:rows,
        		cols:cols,
        		shuffle:false,
        		numbers:false,
        		language:'cn',
        		hole:hole,
        		title:title,
        		control:{
        			toggleNumbers:false,
        			counter:true,
        			timer:true,
        			pauseTimer:true,
        			startcallback:function(r){
        			    if(!window.confirm('活动已结束游戏不再支持色币兑换，但可继续拼!'))
        					 return false;
        				var isstart = false;
        				$.ajax({
        					url:'/ajax/game_puzzle',
        					type:'post',
        					async:false,
        					data:{'title':r.title,'start':r.isstart?1:0},
        					dataType:'json',
        					success:function(r){
        						if(r && r.status < 0){
        							if(r.status == -3){
        								window.location.href = '/login';
        							}
        							alert(r.msg);
        						}else{
        							isstart = {gid:r.gid};
        						}
        					},
        					'fail':function(){
        						isstart = false;
        					}
        				});
        				return isstart;
        			},
        			beforecountercallback:function(r){
        				if(r.seconds <= 0 && !r.gid){
        					alert('请点击开始游戏');
        					return false;
        				}
        				var isplay = true;
        				
        				if(r.moves > 0 && r.seconds - secodes < 1){
        					isplay = false;
        					alert('请不要太急哦，保持一秒一步就好！');
        				}else{
        					secodes = r.seconds;
        				}
        				if(isplay){
	    					$.ajax({
	    						url:'/ajax/game_puzzle',
	        					type:'post',
	        					async:false,
	        					data:{'gid':r.gid,'title':r.title,'moves':r.moves,'seconds':r.seconds,'valid':1},
	        					dataType:'json',
	    						success:function(re){
	    							if(re && re.status < 0){
	    								if(re.status == -3){
	    									window.location.href = '/login';
	    								}
	    								alert(re.msg);
	    								isplay = false;
	    							}
	    						},
	    						'fail':function(){
	    							isplay = false;
	    						}
	    					});
        				}
        				return isplay;
        			},
        			aftercountercallback:function(r){}
        		},
        		animation:{
        			shuffleRounds:rounds
        		},
        		success:{
        			callback:function(r){
        				$.ajax({
        					url:'/ajax/game_puzzle',
        					type:'post',
        					async:false,
        					data:{'gid':r.gid,'title':r.title,'moves':r.moves,'seconds':r.seconds,'isfinish':1},
        					dataType:'json',
        					success:function(re){
        						if(re && re.status < 0){
        								if(re.status == -3){
        									window.location.href = '/login';
        								}
        								alert(re.msg);
        						}else{
        							$('.complete').show().click(function(){
        								window.location.reload();
        							});
        						}
        					},
        					fail:function(){
        					}
        				});
        			}
        		}
        	};
        	$('#pictureList').jqPuzzle(settings);
        });
    {/literal}    
    </script>
