		function createCloseBtn(){
			var $btn = $('<div />');
			$btn.css({'position':'absolute','font-size':'30px','top':'10px','color':'rgb(6, 255, 207)','cursor':'pointer'}).html('X');
			$btn.click(function(){
				$(this).parent().hide();
				//$('#nyhiddenme').parent().show(150);
			});
			return $btn;
		}
		//是否为vip判断
		var vipFlag = 0;
		$.ajax({
			type:"post",
			url:"/ajax.php/ajax_vip",
			dataType:"json",
			cache:false,
			async:false,
			success:function(vip){
                if(vip.code==1){
                    vipFlag = parseInt(vip.msg);
                }
                return vipFlag;
			}
		});
		function showAds(){
			if(vipFlag>1) return;
			if(ads){
				var cWidth = parseInt($('.container').css('width').replace('px',''));
				var ismo = ismobile();
				var pcContainerWidth = ismo ? Math.floor(cWidth *2 + 30) : 1150;
				
				$.each(ads,function(i,r){
					var width = parseInt(r.width) === 0 ? '100%' : Math.floor(r.width * (cWidth / pcContainerWidth))+'px';
					var height = parseInt(r.height)===0 ? '100%' : r.height+'px';
					var advs = $('.ps_'+i);
					if(r.ads && r.ads.length > 0 && advs.length > 0){
						$.each(r.ads,function(ii,rr){
							var url = '';
							var adv = rr;
							if(ismo != parseInt(adv.ismobile)) return;
							$img = $('<img />');
							$imgPath = '';
							if( adv.localpic !=''){
								$imgPath = adv.localpic;
							}else if(adv.media != ''){ 
								$imgPath = adv.media;
							}else if(adv.relogopic != ''){
								$imgPath = adv.relogopic;
							}
							$img.attr('src', $imgPath);
							$img.attr('title', adv.name);
							//手机端清除图片宽度高度属性
							//if(ismo!=1||ismo==1&&parseInt(i)<136){
								$img.attr('style', "width:"+width+";height:"+height+";");
                            //}
							$a = $('<a />');
						
							if(adv.id && adv.relname != '' && adv.relogopic != '') {
								url = '/tiaozhuan.php?id=' + adv.id;
							}else{
								url = (adv.hash) ?'/link/'+adv.hash : adv.url;
							}
							$a.attr('href', url);
							$a.attr('target', '_blank');
							var id = 0;
							if(adv.id){
								id = adv.id;
							}
							var name =  "";
							if(adv.name){
								name =  adv.name;
							}
							var zone_name = "";
							if(adv.zone_name){
								zone_name = adv.zone_name;
							}
							$a.on({
								'click':function(){
									$.post('/ajax/adv_count',{'id':id,'title':name,'zone_name':zone_name},function(resutl){});
								}
							});
                            var margin = '0 0 10px 0'
							if(adv.ismobile==1) var margin = ''//手机端广告间距为空
							if(adv.margin && adv.margin != ''){
								margin = adv.margin;
							}
							if(adv.isbtn && parseInt(adv.isbtn) === 1){
								var btn = createCloseBtn();
								$(advs).append(btn);
							}
							//手机去除DIV宽度
                            if(ismo==1) {
                                $(advs).css({ 'margin': margin}).append($a.append($img));
                            }else{
                                $(advs).css({'width': width, 'margin': margin}).append($a.append($img));
							}
						});
					}
				});
			}
		}
		function HideFoot() {
            var botm = $('.QQ_S').css("top");
            var screenH = $(window).height();
            var len = botm.length;
            num = Number(botm.substr(0, len - 2));
            var time = setInterval(function () {
                if (num <= screenH - 212) {
                    num = num + 40;
                    botmt = num + "px";
                    $('.QQ_S').css("top", botmt)
                } else {
                    clearInterval(time);
                    $('.QQ_S').css("display", "none");
                    $('.QQ_S1').css("display", "block");
                }
            }, 10);
        }
        function ShowFoot() {
            $('.QQ_S').css("display", "block");
            $('.QQ_S1').css("display", "none");
            var botm = $('.QQ_S').css("top");
            var screenH = $('.QQ_none').css("top");
            var len = botm.length; lena = screenH.length;
            num = Number(botm.substr(0, len - 2));
            numa = Number(screenH.substr(0, lena - 2));
            var time = setInterval(function () {
                if (num > numa + 30) {
                    num = num - 30;
                    botmt = num + "px";
                    $('.QQ_S').css("top", botmt);
                } else {
                    $('.QQ_S').css("top", numa + "px");
                    clearInterval(time);
                }
            }, 10);
        } 
